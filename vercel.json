{
  "version": 2,
  "builds": [
    {
      "src": "Server/Program.cs",
      "use": "@vercel/dotnet"
    },
    {
      "src": "Client/wwwroot/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_framework/(.*)",
      "dest": "/_framework/$1"
    },
    {
      "src": "/_content/(.*)",
      "dest": "/_content/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/favicon.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}